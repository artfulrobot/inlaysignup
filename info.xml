<?xml version="1.0"?>
<extension key="inlaysignup" type="module">
  <file>inlaysignup</file>
  <name>Inlay Signup Forms</name>
  <description>Example signup forms for use with CiviCRM Inlay module</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Rich Lott / Artful Robot</author>
    <email>forums@artfulrobot.uk</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/artfulrobot/inlaysignup</url>
    <url desc="Documentation">https://github.com/artfulrobot/inlaysignup</url>
    <url desc="Support">https://github.com/artfulrobot/inlaysignup</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2020-10-19</releaseDate>
  <version>1.0</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>5.27</ver>
  </compatibility>
  <requires>
    <ext>de.systopia.xcm</ext>
  </requires>
  <comments>Example signup forms for use with CiviCRM Inlay module https://civicrm.org/extensions/inlay</comments>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/Inlaysignup</namespace>
    <format>23.02.1</format>
  </civix>
  <mixins>
    <mixin>ang-php@1.0.0</mixin>
  </mixins>
</extension>
